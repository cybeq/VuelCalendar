{
  "name": "vuelcalendar",
  "private": false,
  "version": "0.7.22",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "calendar",
    "vue",
    "vuel",
    "events",
    "scheduling",
    "planning",
    "v-calendar",
    "vue-calendar",
    "eventer",
    "managing events",
    "timeline"
  ],
  "files": [
    "dist"
  ],
  "rules": {
    "no-unused-vars": "off"
  },
  "main": "./dist/vuel-calendar.cjs",
  "module": "./dist/vuel-calendar.js",
  "exports": {
    ".": {
      "import": "./dist/vuel-calendar.js",
      "require": "./dist/vuel-calendar.cjs"
    }
  },
  "dependencies": {
    "vite-plugin-dts": "^3.7.3",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.17",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
